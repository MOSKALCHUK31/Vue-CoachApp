tch('requests/getRequests')\n            } catch (error) {\n                this.error = error\n            }\n\n            this.isLoading = false\n        },\n        handleCloseModal() {\n            this.error = null\n        }\n    },\n    created() {\n        this.loadRequests()\n    }\n}\n</script>\n\n<style scoped>\nheader {\n    text-align: center;\n}\n\nul {\n    list-style: none;\n    margin: 2rem auto;\n    padding: 0;\n    max-width: 30rem;\n}\n\nh3 {\n    text-align: center;\n}\n</style>"]}]}