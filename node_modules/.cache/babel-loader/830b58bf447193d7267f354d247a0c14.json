I,KAAI;IACzB,CAAC;IACDM,gBAAgB,GAAG;MACf,IAAI,CAACL,KAAI,GAAI,IAAG;IACpB;EACJ,CAAC;EACDM,OAAO,GAAG;IACN,IAAI,CAACJ,YAAY,EAAC;EACtB;AACJ","names":["mapGetters","RequestItem","components","computed","getRequests","getHasRequests","receivedRequests","hasRequests","data","isLoading","error","methods","loadRequests","$store","dispatch","handleCloseModal","created"],"sourceRoot":"","sources":["/Users/moskalchuk/Documents/FrontEnd/projects/Vue/Vue [CoachApp]/src/pages/requests/RequestsReceived.vue"],"sourcesContent":["<template>\n    <div>\n        <base-dialog \n            :show=\"!!error\" \n            title=\"Opps! Something went wrong\" \n            @close=\"handleCloseModal\"\n        >\n            <p>{{ error }}</p>\n        </base-dialog>\n        <section>\n            <base-card>\n                <header>\n                    <h2>Requests Received</h2>\n                </header>\n                <ul v-if=\"hasRequests && !isLoading\">\n                    <request-item\n                        v-for=\"request in receivedRequests\"\n                        :key=\"request.id\"\n                        :email=\"request.email\"\n                        :message=\"request.message\"\n                    ></request-item>\n                </ul>\n                <base-spinner v-else-if=\"isLoading\"></base-spinner>\n                <h3 v-else>You haven't received any requests yet!</h3>\n            </base-card>\n        </section>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport RequestItem from '../../components/requests/RequestItem.vue'\n\nexport default {\n    components: {\n        RequestItem\n    },\n    computed: {\n        ...mapGetters({\n            getRequests: 'requests/getRequests',\n            getHasRequests: 'requests/hasRequests'\n        }),\n        receivedRequests() {\n            return this.getRequests\n        },\n        hasRequests() {\n            return this.getHasRequests\n        }\n    },\n    data() {\n        return {\n            isLoading: false,\n            error: null\n        }\n    },\n    methods: {\n        async loadRequests() {\n            this.isLoading = true\n            \n            try {\n                await this.$store.dispatch('requests/getRequests')\n            } catch (error) {\n                this.error = error\n            }\n\n            this.isLoading = false\n        },\n        handleCloseModal() {\n            this.error = null\n        }\n    },\n    created() {\n        this.loadRequests()\n    }\n}\n</script>\n\n<style scoped>\nheader {\n    text-align: center;\n}\n\nul {\n    list-style: none;\n    margin: 2rem auto;\n    padding: 0;\n    max-width: 30rem;\n}\n\nh3 {\n    text-align: center;\n}\n</style>"]}]}